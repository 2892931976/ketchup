{
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "always", "strict-bound-class-methods"],
    "no-console": [true, "log", "warn"],
    "trailing-comma": [true, "never"],
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "(^_|msx)"
      }
    ],
    "arrow-parens": true,
    "eofline": true
  }
}
